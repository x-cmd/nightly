____x_cmd_jo_query(){
    local IFS="$___X_CMD_UNSEENCHAR_001"
    {
        printf "%s" "$*"
        printf "%s" "---"
        cat
    } | awk \
        -f "$___X_CMD_ROOT_MOD/awk/lib/default.awk" \
        -f "$___X_CMD_ROOT_MOD/awk/lib/json.awk" \
        -f "$___X_CMD_ROOT_MOD/awk/lib/jiter.awk"
}