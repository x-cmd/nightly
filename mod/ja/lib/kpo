
___x_cmd_ja_kpof(){
    local IFS=' '
    ___x_cmd_ja_tokenized | \
    command awk \
        -f "$___X_CMD_ROOT_MOD/ja/lib/awk/op.awk" \
        -f "$___X_CMD_ROOT_MOD/ja/lib/awk/kpo.awk" \
        -f "$1"
}


___x_cmd_ja_kpo(){
    local IFS=' '
    ___x_cmd_ja_tokenized | \
    command awk "$(cat "$___X_CMD_ROOT_MOD/ja/lib/awk/op.awk" "$___X_CMD_ROOT_MOD/ja/lib/awk/kpo.awk")"'

'"
$*
"

}
