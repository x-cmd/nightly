. ./latest

___x_cmd_jo_fmt(){
    command awk -v INDENT="${INDENT:-2}}" -f "./lib/fmt.awk"
}

___x_cmd_jo_color(){
    command awk -v INDENT="${INDENT:-2}}" -f "./lib/color.awk"
}
___x_cmd_jo_awk(){  command awk -f "../awk/lib/re.awk" -f "../awk/lib/jo.awk";    }





test_jo(){
    ___x_cmd_jo '[1, 2 3 {a: a, a1:123,abv:[1 2 3] ,d:{d1:12, d2:asc,d3:true , d4:{e1:true,2e:null}}}  ]'
}
test_list(){
    ___x_cmd_jo list 1 2 3 aaa a1 2b c3c3c
}
test_dict(){
    local abc=123
    local cde="test"
    local efg="frue"
    local hij=true
    ___x_cmd_jo dict a=12 b:12 b:cscs  abc cde efg e1=null e2=false e3=true hij
}

case "$1" in
    list|dict)      test_${1} ;;
    *)              test_jo ;;
esac